{
	"name": "dereuromark/cakephp-ratings",
	"type": "cakephp-plugin",
	"description": "Ratings Plugin for CakePHP.",
	"homepage": "https://github.com/dereuromark/cakephp-ratings",
	"keywords": ["cakephp", "ratings", "plugin"],
	"license": "MIT",
	"authors": [
		{
			"name": "Mark Scherer",
			"email": "euromark@web.de",
			"homepage": "https://github.com/dereuromark/cakephp-ratings",
			"role": "maintainer"
		}, {
			"name": "Cake Development Corporation",
			"email": "team@cakedc.com",
			"homepage": "https://cakedc.com",
			"role": "original maintainer"
		}
	],
	"support": {
		"issues": "https://github.com/dereuromark/cakephp-ratings/issues",
		"wiki": "https://github.com/dereuromark/cakephp-ratings/blob/master/docs",
		"source": "https://github.com/dereuromark/cakephp-ratings"
	},
	"require":{
		"php": ">=7.3",
		"cakephp/cakephp": "^4.2.0"
	},
	"require-dev": {
		"dereuromark/cakephp-shim": "^2.0.0",
		"phpunit/phpunit": "^9.5",
		"fig-r/psr2r-sniffer": "dev-master"
	},
	"autoload": {
		"psr-4": {
			"Ratings\\": "src/",
			"Ratings\\Test\\Fixture\\": "tests/Fixture/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Ratings\\Test\\": "tests/",
			"Cake\\Test\\": "vendor/cakephp/cakephp/tests/",
			"TestApp\\": "tests/test_app/src/"
		}
	},
	"suggest": {
		"cakephp/migrations": "For migrations to run."
	},
	"scripts": {
		"test": "phpunit",
		"test-coverage": "phpunit --log-junit tmp/coverage/unitreport.xml --coverage-html tmp/coverage --coverage-clover tmp/coverage/coverage.xml",
		"stan": "phpstan analyse",
		"stan-setup": "cp composer.json composer.backup && composer require --dev phpstan/phpstan:^1.0.0 && mv composer.backup composer.json",
		"cs-check": "phpcs -p -s --standard=vendor/fig-r/psr2r-sniffer/PSR2R/ruleset.xml --ignore=/config/Migrations/ --extensions=php src/ tests/ config/",
		"cs-fix": "phpcbf -p --standard=vendor/fig-r/psr2r-sniffer/PSR2R/ruleset.xml --ignore=/config/Migrations/ --extensions=php src/ tests/ config/"
	}
}
